!/bin/bash

echo "Welcome to the Step Yo Game Up script installer
read -p "Do you want to step your game up?

If [ "$install_choice" == "yes"  ];
then
	echo "That's a bet! Let me hook you up."
	 sudo apt-get install -y apache2 mysql-server gnupq curl
	.
else
	
	echo "It's all good. Maybe next time."
	.
fi


sudo apt list --installed
sudo apt list --upgradeable
